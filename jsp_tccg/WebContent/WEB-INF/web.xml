<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>/qiantai/default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>SetChartEncoding</filter-name>
    <filter-class>com.util.EncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SetChartEncoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>dwr-invoker</servlet-name>
    <servlet-class>
			org.directwebremoting.servlet.DwrServlet
		</servlet-class>
    <init-param>
      <param-name>debug</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>activeReverseAjaxEnabled</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>initApplicationScopeCreatorsAtStartup</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>maxWaitAfterWrite</param-name>
      <param-value>500</param-value>
    </init-param>
    <init-param>
      <param-name>crossDomainSessionSecurity</param-name>
      <param-value>false</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>dwr-invoker</servlet-name>
    <url-pattern>/dwr/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>com.action.index_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>com.action.user_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>zhengce</servlet-name>
    <servlet-class>com.action.zhengce_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>zhengce</servlet-name>
    <url-pattern>/zhengce</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>chewei</servlet-name>
    <servlet-class>com.action.chewei_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>chewei</servlet-name>
    <url-pattern>/chewei</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ting</servlet-name>
    <servlet-class>com.action.ting_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ting</servlet-name>
    <url-pattern>/ting</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>liuyan</servlet-name>
    <servlet-class>com.action.liuyan_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>liuyan</servlet-name>
    <url-pattern>/liuyan</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>yuding</servlet-name>
    <servlet-class>com.action.yuding_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>yuding</servlet-name>
    <url-pattern>/yuding</url-pattern>
  </servlet-mapping>
</web-app>